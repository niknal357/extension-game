{
    "manifest_version": 3,
    "name": "Gnome Game",
    "version": "0.3",
    "action": {
        "default_popup": "index.html"
    },
    "description": "A gnome game made by Nikita Lurye, Maxime Tschopp, and Manzoh Gellner",
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "web_accessible_resources": [
        {
            "resources": ["save.json"],
            "matches": ["<all_urls>"],
            "extension_ids": []
        }
    ],
    "permissions": ["storage", "unlimitedStorage"],
    "host_permissions": ["<all_urls>"]
}
