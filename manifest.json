{
    "manifest_version": 3,
    "name": "Platformer Game",
    "version": "1.0.0",
    "action": {
        "default_popup": "index.html"
    },
    "description": "A platformer game",
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "content_scripts": [
        {
            "matches": ["https://powerschool.zis.ch/public/"],
            "js": ["inject.js"]
        }
    ],
    "host_permissions": ["<all_urls>"]
}